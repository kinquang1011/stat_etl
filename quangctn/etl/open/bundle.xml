<bundle-app name="UB :: SPARK :: ${logType} :: QUANGCTN  - Bundle" xmlns="uri:oozie:bundle:0.2">
    <parameters>
        <property><name>coordTimeZone</name><value>Asia/Ho_Chi_Minh</value></property>
        <property><name>coordTimeout</name><value>720</value></property>
        <property><name>coordConcurrency</name><value>1</value></property>
        <property><name>startDate</name><value>${startDate}</value></property>
        <property><name>endDate</name><value>${endDate}</value></property>
        <property><name>frequency</name><value>1</value></property>
        <property><name>doneFlag</name><value>_SUCCESS</value></property>
        <property><name>sparkMaster</name><value>yarn-cluster</value></property>
        <property><name>statsSparkJar</name><value>stats-etlr-1.0.jar</value></property>
        <property><name>sparkClass</name><value>vng.ge.stats.etl.transform.Factory</value></property>
    </parameters>

    <controls>
        <kick-off-time>${kickOffTime}</kick-off-time>
    </controls>

    <coordinator name="UB :: SPARK :: PNTT - Daily ETL Coord">
        <app-path>${bundleApplicationPath}/coordinator2.xml</app-path>
        <configuration>
            <property><name>className</name><value>Pntt</value></property>
            <property><name>gameCode</name><value>pnttmobi</value></property>
        </configuration>
    </coordinator>
    <coordinator name="UB :: SPARK :: MUW - Daily ETL Coord">
        <app-path>${bundleApplicationPath}/coordinator2.xml</app-path>
        <configuration>
            <property><name>className</name><value>Muw</value></property>
            <property><name>gameCode</name><value>muw</value></property>
        </configuration>
    </coordinator>

    <coordinator name="UB :: SPARK :: Tfirb2s - Daily ETL Coord">
        <app-path>${bundleApplicationPath}/coordinator2.xml</app-path>
        <configuration>
            <property><name>className</name><value>Tfirb2s</value></property>
            <property><name>gameCode</name><value>tfirb2s</value></property>
        </configuration>
    </coordinator>
    <coordinator name="UB :: SPARK :: Dt2 - Daily ETL Coord">
        <app-path>${bundleApplicationPath}/coordinator.xml</app-path>
        <configuration>
            <property><name>className</name><value>Dt2</value></property>
            <property><name>gameCode</name><value>zombie3d2m</value></property>
        </configuration>
    </coordinator>
